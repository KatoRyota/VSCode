{
    "editor.minimap.enabled": false,
    "editor.maxTokenizationLineLength": 10000000,
    "editor.find.autoFindInSelection": "multiline",
    "editor.find.seedSearchStringFromSelection": "selection",
    "editor.renderWhitespace": "all",
    "editor.showFoldingControls": "always",
    "editor.rulers": [
        60,
        80,
        120
    ],
    "search.showLineNumbers": true,
    "workbench.editor.doubleClickTabToToggleEditorGroupSizes": "maximize",
    "workbench.editor.enablePreview": false,
    "git.autofetch": true,
    "diffEditor.renderSideBySideInlineBreakpoint": 1,
    "diffEditor.ignoreTrimWhitespace": true,
    "diffEditor.hideUnchangedRegions.enabled": false,
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "terminal.integrated.allowChords": false,
    "terminal.integrated.sendKeybindingsToShell": true,
    "terminal.integrated.stickyScroll.enabled": false,
    "npm.exclude": "",
    "git-log--graph.details-panel-position": "bottom",
    "githd.logView.expressMode": true,
    "githd.blameView.enabled": false,
    "markdown-preview-enhanced.previewMode": "Multiple Previews",
    "markdowntable.showMenu.csvToTable": true,
    "sqldeveloper.sqlHistory.historyLimit": 500,
    "sqldeveloper.telemetry.enabled": false,
    "github.copilot.nextEditSuggestions.enabled": true,
    "github.copilot.enable": {
        "plaintext": true,
        "markdown": true
    },
    "maptz.regionfolder": {
        "[plaintext]": {
            "foldStart": "#region [NAME]",
            "foldEnd": "#endregion",
            "foldStartRegex": "#region\\s*(.*)",
            "foldEndRegex": "#endregion",
            "defaultFoldStartRegex": "#region\\b",
            "disableFolding": false
        },
        "[markdown]": {
            "foldStart": "<!-- #region [NAME] -->",
            "foldEnd": "<!-- #endregion -->",
            "foldStartRegex": "<!--\\s*#region\\s*(.*)-->",
            "foldEndRegex": "<!--\\s*#endregion\\s*-->",
            "defaultFoldStartRegex": "<!--\\s*#region\\b.*-->",
            "disableFolding": false
        }
    },
    "commands.commands": {
        "表示": {
            "nestedItems": {
                "表示: エディター グループのサイズをリセット 【F13】": {
                    "command": "workbench.action.evenEditorWidths",
                    "icon": "window",
                    "iconColor": "terminal.ansiBlue"
                },
                "表示: エディター グループの最大化の切り替え 【F14】": {
                    "command": "workbench.action.toggleMaximizeEditorGroup",
                    "icon": "window",
                    "iconColor": "terminal.ansiBlue"
                },
                "表示: プライマリ サイド バーの表示/非表示を切り替える 【F15】": {
                    "command": "workbench.action.toggleSidebarVisibility",
                    "icon": "window",
                    "iconColor": "terminal.ansiBlue"
                },
                "表示: セカンダリ サイド バーの表示/非表示を切り替える 【F16】": {
                    "command": "workbench.action.toggleAuxiliaryBar",
                    "icon": "window",
                    "iconColor": "terminal.ansiBlue"
                },
                "表示: パネルの表示/非表示の切り替え 【F17】": {
                    "command": "workbench.action.togglePanel",
                    "icon": "window",
                    "iconColor": "terminal.ansiBlue"
                },
                "表示: [右端での折り返し] の設定/解除 【F18】": {
                    "command": "editor.action.toggleWordWrap",
                    "icon": "window",
                    "iconColor": "terminal.ansiBlue"
                },
                "表示: エディターの分割 【F19】": {
                    "command": "workbench.action.splitEditor",
                    "icon": "window",
                    "iconColor": "terminal.ansiBlue"
                },
                "レベル 1 で折りたたむ": {
                    "command": "editor.foldLevel1",
                    "icon": "window",
                    "iconColor": "terminal.ansiBlue"
                },
                "レベル 2 で折りたたむ": {
                    "command": "editor.foldLevel2",
                    "icon": "window",
                    "iconColor": "terminal.ansiBlue"
                },
                "レベル 3 で折りたたむ": {
                    "command": "editor.foldLevel3",
                    "icon": "window",
                    "iconColor": "terminal.ansiBlue"
                },
                "レベル 4 で折りたたむ": {
                    "command": "editor.foldLevel4",
                    "icon": "window",
                    "iconColor": "terminal.ansiBlue"
                },
                "レベル 5 で折りたたむ": {
                    "command": "editor.foldLevel5",
                    "icon": "window",
                    "iconColor": "terminal.ansiBlue"
                },
                "選択した項目を除くすべて折りたたみ": {
                    "command": "editor.foldAllExcept",
                    "icon": "window",
                    "iconColor": "terminal.ansiBlue"
                },
                "選択した項目を除くすべて展開": {
                    "command": "editor.unfoldAllExcept",
                    "icon": "window",
                    "iconColor": "terminal.ansiBlue"
                }
            }
        },
        "ファイル": {
            "nestedItems": {
                "選択範囲から折りたたみ範囲を作成する 【F20】": {
                    "command": "editor.createFoldingRangeFromSelection",
                    "icon": "file-code",
                    "iconColor": "terminal.ansiGreen"
                },
                "手動折りたたみ範囲を削除する 【F21】": {
                    "command": "editor.removeManualFoldingRanges",
                    "icon": "file-code",
                    "iconColor": "terminal.ansiGreen"
                },
                "ファイル: セッションでのアクティブ エディターの読み取り専用の切り替え 【F22】": {
                    "command": "workbench.action.files.toggleActiveEditorReadonlyInSession",
                    "icon": "file-code",
                    "iconColor": "terminal.ansiGreen"
                },
                "Markdown: ロックされたプレビューを横に表示": {
                    "command": "markdown.showLockedPreviewToSide",
                    "icon": "file-code",
                    "iconColor": "terminal.ansiGreen"
                },
                "GitHD: View Branch Diff": {
                    "command": "githd.diffBranch",
                    "icon": "file-code",
                    "iconColor": "terminal.ansiGreen"
                }
            }
        },
        "移動": {
            "nestedItems": {
                "最後の編集位置へ移動 【Ctrl+F13】": {
                    "command": "workbench.action.navigateToLastEditLocation",
                    "icon": "go-to-file",
                    "iconColor": "terminal.ansiRed"
                },
                "ブラケットへ移動 【Ctrl+F14】": {
                    "command": "editor.action.jumpToBracket",
                    "icon": "go-to-file",
                    "iconColor": "terminal.ansiRed"
                },
                "行/列に移動... 【Ctrl+F15】": {
                    "command": "workbench.action.gotoLine",
                    "icon": "go-to-file",
                    "iconColor": "terminal.ansiRed"
                },
                "Center Editor Window 【Ctrl+F16】": {
                    "command": "center-editor-window.center",
                    "icon": "go-to-file",
                    "iconColor": "terminal.ansiRed"
                }
            }
        },
        "選択": {
            "nestedItems": {
                "選択範囲を拡張 【Ctrl+F17】": {
                    "command": "editor.action.smartSelect.expand",
                    "icon": "selection",
                    "iconColor": "terminal.ansiMagenta"
                },
                "選択範囲を縮小 【Ctrl+F18】": {
                    "command": "editor.action.smartSelect.shrink",
                    "icon": "selection",
                    "iconColor": "terminal.ansiMagenta"
                },
                "列選択モードの切り替え 【Ctrl+F19】": {
                    "command": "editor.action.toggleColumnSelection",
                    "icon": "selection",
                    "iconColor": "terminal.ansiMagenta"
                }
            }
        },
        "編集": {
            "nestedItems": {
                "置換 【Shift+F13】": {
                    "command": "editor.action.startFindReplaceAction",
                    "icon": "edit",
                    "iconColor": "terminal.ansiYellow"
                },
                "選択範囲の複製 【Shift+F14】": {
                    "command": "editor.action.duplicateSelection",
                    "icon": "edit",
                    "iconColor": "terminal.ansiYellow"
                },
                "小文字に変換 【Shift+F15】": {
                    "command": "editor.action.transformToLowercase",
                    "icon": "edit",
                    "iconColor": "terminal.ansiYellow"
                },
                "大文字に変換 【Shift+F16】": {
                    "command": "editor.action.transformToUppercase",
                    "icon": "edit",
                    "iconColor": "terminal.ansiYellow"
                },
                "行をつなげる 【Shift+F17】": {
                    "command": "editor.action.joinLines",
                    "icon": "edit",
                    "iconColor": "terminal.ansiYellow"
                },
                "行コメントの切り替え 【Shift+F18】": {
                    "command": "editor.action.commentLine",
                    "icon": "edit",
                    "iconColor": "terminal.ansiYellow"
                },
                "ブロック コメントの切り替え 【Shift+F19】": {
                    "command": "editor.action.blockComment",
                    "icon": "edit",
                    "iconColor": "terminal.ansiYellow"
                },
                "ドキュメントのフォーマット 【Shift+F20】": {
                    "command": "editor.action.formatDocument",
                    "icon": "edit",
                    "iconColor": "terminal.ansiYellow"
                },
                "選択範囲のフォーマット 【Shift+F21】": {
                    "command": "editor.action.formatSelection",
                    "icon": "edit",
                    "iconColor": "terminal.ansiYellow"
                },
                "Region folding: Wrap selection with #region tag.": {
                    "command": "regionfolder.wrapWithRegion",
                    "icon": "edit",
                    "iconColor": "terminal.ansiYellow"
                },
                "Region folding: Delete current #region tags": {
                    "command": "regionfolder.removeCurrentRegionTags",
                    "icon": "edit",
                    "iconColor": "terminal.ansiYellow"
                },
                "Markdown All in One: Create Table of Contents": {
                    "command": "markdown.extension.toc.create",
                    "icon": "edit",
                    "iconColor": "terminal.ansiYellow"
                },
                "Markdown All in One: Add/Update section numbers": {
                    "command": "markdown.extension.toc.addSecNumbers",
                    "icon": "edit",
                    "iconColor": "terminal.ansiYellow"
                },
                "Markdown All in One: Remove section numbers": {
                    "command": "markdown.extension.toc.removeSecNumbers",
                    "icon": "edit",
                    "iconColor": "terminal.ansiYellow"
                },
                "Markdown All in One: Toggle list": {
                    "command": "markdown.extension.editing.toggleList",
                    "icon": "edit",
                    "iconColor": "terminal.ansiYellow"
                },
                "Markdown All in One: Toggle code block": {
                    "command": "markdown.extension.editing.toggleCodeBlock",
                    "icon": "edit",
                    "iconColor": "terminal.ansiYellow"
                },
                "Markdown Table: Format all tables.": {
                    "command": "markdowntable.format",
                    "icon": "edit",
                    "iconColor": "terminal.ansiYellow"
                },
                "Markdown Table: Insert column to the left.": {
                    "command": "markdowntable.insertLeft",
                    "icon": "edit",
                    "iconColor": "terminal.ansiYellow"
                },
                "Markdown Table: Insert column to the right.": {
                    "command": "markdowntable.insertRight",
                    "icon": "edit",
                    "iconColor": "terminal.ansiYellow"
                },
                "Markdown Table: Move to Left.": {
                    "command": "markdowntable.moveLeft",
                    "icon": "edit",
                    "iconColor": "terminal.ansiYellow"
                },
                "Markdown Table: Move to Right.": {
                    "command": "markdowntable.moveRight",
                    "icon": "edit",
                    "iconColor": "terminal.ansiYellow"
                },
                "Markdown Table: Align to Center.": {
                    "command": "markdowntable.alignCenter",
                    "icon": "edit",
                    "iconColor": "terminal.ansiYellow"
                },
                "Markdown Table: Align to Left.": {
                    "command": "markdowntable.alignLeft",
                    "icon": "edit",
                    "iconColor": "terminal.ansiYellow"
                },
                "Markdown Table: Align to Right.": {
                    "command": "markdowntable.alignRight",
                    "icon": "edit",
                    "iconColor": "terminal.ansiYellow"
                },
                "Markdown Table: Convert CSV to table.": {
                    "command": "markdowntable.csvToTable",
                    "icon": "edit",
                    "iconColor": "terminal.ansiYellow"
                },
                "Markdown Table: Convert TSV to table.": {
                    "command": "markdowntable.tsvToTable",
                    "icon": "edit",
                    "iconColor": "terminal.ansiYellow"
                }
            }
        },
        "ターミナル": {
            "nestedItems": {
                "ターミナル: アクティブなターミナルで選択したテキストを実行 【Shift+F22】": {
                    "command": "workbench.action.terminal.runSelectedText",
                    "icon": "terminal",
                    "iconColor": "terminal.ansiBrightMagenta"
                },
                "ls-find: 特定ディレクトリ配下の一覧表示（ショート版）": {
                    "sequence": [
                        "workbench.action.terminal.focus",
                        {
                            "command": "workbench.action.terminal.sendSequence",
                            "args": {
                                "text": "find $DIR -type f -exec ls -1FA --block-size=M --time-style='+%Y-%m-%dT%H:%M:%S' {} +"
                            }
                        }
                    ],
                    "icon": "terminal",
                    "iconColor": "terminal.ansiBrightMagenta"
                },
                "ls-find: 特定ディレクトリ配下の一覧表示（ロング版）": {
                    "sequence": [
                        "workbench.action.terminal.focus",
                        {
                            "command": "workbench.action.terminal.sendSequence",
                            "args": {
                                "text": "find $DIR -type f -exec ls -lFA --block-size=M --time-style='+%Y-%m-%dT%H:%M:%S' {} +"
                            }
                        }
                    ],
                    "icon": "terminal",
                    "iconColor": "terminal.ansiBrightMagenta"
                },
                "grep-find: 特定ディレクトリ配下の検索": {
                    "sequence": [
                        "workbench.action.terminal.focus",
                        {
                            "command": "workbench.action.terminal.sendSequence",
                            "args": {
                                "text": "find $DIR \\( -path '.git' $EXCLUDE_PATTERN \\) -prune -o -type f -exec grep -anHPi $SEARCH_PATTERN {} +"
                            }
                        }
                    ],
                    "icon": "terminal",
                    "iconColor": "terminal.ansiBrightMagenta"
                },
                "diff: ディレクトリ間の差分比較（Windows）": {
                    "sequence": [
                        "workbench.action.terminal.focus",
                        {
                            "command": "workbench.action.terminal.sendSequence",
                            "args": {
                                "text": "diff -qr -x \"$EXCLUDE_PATTERN\" $(cygpath -u \"$DIR_1\") $(cygpath -u \"$DIR_2\")"
                            }
                        }
                    ],
                    "icon": "terminal",
                    "iconColor": "terminal.ansiBrightMagenta"
                },
                "cygpath: WindowsのパスをUnix形式に変換": {
                    "sequence": [
                        "workbench.action.terminal.focus",
                        {
                            "command": "workbench.action.terminal.sendSequence",
                            "args": {
                                "text": "cygpath -u \"$WIN_PATH\""
                            }
                        }
                    ],
                    "icon": "terminal",
                    "iconColor": "terminal.ansiBrightMagenta"
                },
                "scp: リモート→踏み台": {
                    "sequence": [
                        "workbench.action.terminal.focus",
                        {
                            "command": "workbench.action.terminal.sendSequence",
                            "args": {
                                "text": "scp $SRC_PATH $USER@$DEST_HOST:$DEST_PATH"
                            }
                        }
                    ],
                    "icon": "terminal",
                    "iconColor": "terminal.ansiBrightMagenta"
                },
                "scp: 踏み台→ローカル": {
                    "sequence": [
                        "workbench.action.terminal.focus",
                        {
                            "command": "workbench.action.terminal.sendSequence",
                            "args": {
                                "text": "scp $USER@$SRC_HOST:$SRC_PATH ~/Downloads/"
                            }
                        }
                    ],
                    "icon": "terminal",
                    "iconColor": "terminal.ansiBrightMagenta"
                }
            }
        },
        "ターミナル.SSHログイン": {
            "nestedItems": {
                "ssh: 踏み台サーバー": {
                    "sequence": [
                        "workbench.action.terminal.focus",
                        {
                            "command": "workbench.action.terminal.sendSequence",
                            "args": {
                                "text": "ssh $USER@$REMOTE_HOST"
                            }
                        }
                    ],
                    "icon": "terminal",
                    "iconColor": "terminal.ansiBrightGreen"
                },
                "ssh: リモートサーバー-01": {
                    "sequence": [
                        "workbench.action.terminal.focus",
                        {
                            "command": "workbench.action.terminal.sendSequence",
                            "args": {
                                "text": "ssh $REMOTE_HOST"
                            }
                        }
                    ],
                    "icon": "terminal",
                    "iconColor": "terminal.ansiBrightGreen"
                },
                "ssh: リモートサーバー-02": {
                    "sequence": [
                        "workbench.action.terminal.focus",
                        {
                            "command": "workbench.action.terminal.sendSequence",
                            "args": {
                                "text": "ssh $REMOTE_HOST"
                            }
                        }
                    ],
                    "icon": "terminal",
                    "iconColor": "terminal.ansiBrightGreen"
                }
            }
        }
    }
}