{
    "editor.minimap.enabled": false,
    "editor.maxTokenizationLineLength": 10000000,
    "editor.find.autoFindInSelection": "multiline",
    "editor.find.seedSearchStringFromSelection": "selection",
    "editor.renderWhitespace": "all",
    "editor.showFoldingControls": "always",
    "editor.rulers": [60, 80, 120],
    "editor.defaultFormatter": "prettier.prettier-vscode",
    "editor.detectIndentation": false,
    "search.showLineNumbers": true,
    "workbench.editor.doubleClickTabToToggleEditorGroupSizes": "maximize",
    "workbench.iconTheme": "vscode-icons",
    "window.customMenuBarAltFocus": false,
    "keyboard.mapAltGrToCtrlAlt": true,
    "git.autofetch": true,
    "git.pruneOnFetch": true,
    "diffEditor.renderSideBySideInlineBreakpoint": 1,
    "diffEditor.ignoreTrimWhitespace": true,
    "diffEditor.hideUnchangedRegions.enabled": false,
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "terminal.integrated.allowChords": false,
    "terminal.integrated.sendKeybindingsToShell": true,
    "terminal.integrated.stickyScroll.enabled": false,
    "terminal.integrated.altClickMovesCursor": false,
    "[oracle-sql]": {
        "editor.defaultFormatter": "Oracle.sql-developer"
    },
    "[sql]": {
        "editor.defaultFormatter": "ReneSaarsoo.sql-formatter-vsc"
    },
    "npm.exclude": "",
    "github.copilot.nextEditSuggestions.enabled": true,
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": false,
        "scminput": false
    },
    "maptz.regionfolder": {
        "[plaintext]": {
            "foldStart": "#region [NAME]",
            "foldEnd": "#endregion",
            "foldStartRegex": "#region\\s*(.*)",
            "foldEndRegex": "#endregion",
            "defaultFoldStartRegex": "#region\\b",
            "disableFolding": false
        },
        "[markdown]": {
            "foldStart": "<!-- #region [NAME] -->",
            "foldEnd": "<!-- #endregion -->",
            "foldStartRegex": "<!--\\s*#region\\s*(.*)-->",
            "foldEndRegex": "<!--\\s*#endregion\\s*-->",
            "defaultFoldStartRegex": "<!--\\s*#region\\b.*-->",
            "disableFolding": false
        }
    },
    "git-log--graph.position": "view",
    "git-log--graph.group-branch-remotes": false,
    "git-log--graph.disable-commit-stats": true,
    "githd.logView.expressMode": true,
    "githd.blameView.enabled": "disabled",
    "githd.editor.disabled": true,
    "githd.logView.displayExpressStatus": false,
    "prettier.tabWidth": 4,
    "SQL-Formatter-VSCode.dialect": "plsql",
    "SQL-Formatter-VSCode.ignoreTabSettings": true,
    "SQL-Formatter-VSCode.tabSizeOverride": 4,
    "SQL-Formatter-VSCode.linesBetweenQueries": 2,
    "markdown-preview-enhanced.previewMode": "Multiple Previews",
    "markdowntable.showMenu.csvToTable": true,
    "sqldeveloper.sqlHistory.historyLimit": 500,
    "sqldeveloper.telemetry.enabled": false,
    "sqldeveloper.format.alignment": {
        "rightAlignQueryKeywords": false,
        "namedArgumentSeparator": false
    },
    "sqldeveloper.format.indentation.identSpaces": 4,
    "redhat.telemetry.enabled": false,
    "java.settings.url": "https://gist.github.com/snjeza/e59f0ce031f237a9d0f4f2aec404a4bb",
    "java.format.settings.url": "https://raw.githubusercontent.com/google/styleguide/gh-pages/eclipse-java-google-style.xml",
    "java.format.settings.profile": "GoogleStyle",
    "AHK++.v2.file": {
        "maxScanDepth": 2
    },
    "AHK++.v2.formatter": {
        "maxPreserveNewlines": 2,
        "wrapLineLength": 120,
        "objectStyle": "expand",
        "arrayStyle": "expand"
    },
    "commands.commands": {
        "表示.全般": {
            "nestedItems": {
                "表示: [右端での折り返し] の設定/解除 【F13】": {
                    "command": "editor.action.toggleWordWrap",
                    "icon": "gear",
                    "iconColor": "terminal.ansiBrightYellow",
                    "disableTooltip": true
                },
                "表示: エディターの分割": {
                    "command": "workbench.action.splitEditor",
                    "icon": "gear",
                    "iconColor": "terminal.ansiBrightYellow",
                    "disableTooltip": true,
                    "hidden": true
                }
            }
        },
        "表示.レイアウト": {
            "nestedItems": {
                "表示: エディター グループのサイズをリセット 【F14】": {
                    "command": "workbench.action.evenEditorWidths",
                    "icon": "layout",
                    "iconColor": "terminal.ansiBlue",
                    "disableTooltip": true
                },
                "表示: エディター グループの最大化の切り替え": {
                    "command": "workbench.action.toggleMaximizeEditorGroup",
                    "icon": "layout",
                    "iconColor": "terminal.ansiBlue",
                    "disableTooltip": true,
                    "hidden": true
                },
                "表示: プライマリ サイド バーの表示/非表示を切り替える": {
                    "command": "workbench.action.toggleSidebarVisibility",
                    "icon": "layout",
                    "iconColor": "terminal.ansiBlue",
                    "disableTooltip": true,
                    "hidden": true
                },
                "表示: セカンダリ サイド バーの表示/非表示を切り替える": {
                    "command": "workbench.action.toggleAuxiliaryBar",
                    "icon": "layout",
                    "iconColor": "terminal.ansiBlue",
                    "disableTooltip": true,
                    "hidden": true
                },
                "表示: パネルの表示/非表示の切り替え": {
                    "command": "workbench.action.togglePanel",
                    "icon": "layout",
                    "iconColor": "terminal.ansiBlue",
                    "disableTooltip": true,
                    "hidden": true
                }
            }
        },
        "表示.スクロール": {
            "nestedItems": {
                "行の位置: トップ 【F15】": {
                    "command": "revealLine",
                    "args": {
                        "lineNumber": "${lineNumber}",
                        "at": "top"
                    },
                    "icon": "go-to-file",
                    "iconColor": "terminal.ansiBrightCyan",
                    "disableTooltip": true
                },
                "行の位置: センター 【F16】": {
                    "command": "revealLine",
                    "args": {
                        "lineNumber": "${lineNumber}",
                        "at": "center"
                    },
                    "icon": "go-to-file",
                    "iconColor": "terminal.ansiBrightCyan",
                    "disableTooltip": true
                }
            }
        },
        "表示.折りたたみ": {
            "nestedItems": {
                "選択した項目を除くすべて折りたたみ 【F17】": {
                    "command": "editor.foldAllExcept",
                    "icon": "symbol-class",
                    "iconColor": "terminal.ansiMagenta",
                    "disableTooltip": true
                },
                "選択した項目を除くすべて展開 【F18】": {
                    "command": "editor.unfoldAllExcept",
                    "icon": "symbol-class",
                    "iconColor": "terminal.ansiMagenta",
                    "disableTooltip": true
                },
                "選択範囲から折りたたみ範囲を作成する 【F19】": {
                    "command": "editor.createFoldingRangeFromSelection",
                    "icon": "symbol-class",
                    "iconColor": "terminal.ansiMagenta",
                    "disableTooltip": true
                },
                "手動折りたたみ範囲を削除する 【F20】": {
                    "command": "editor.removeManualFoldingRanges",
                    "icon": "symbol-class",
                    "iconColor": "terminal.ansiMagenta",
                    "disableTooltip": true
                }
            }
        },
        "ファイル.全般": {
            "nestedItems": {
                "ファイル: セッションでのアクティブ エディターの読み取り専用の切り替え": {
                    "command": "workbench.action.files.toggleActiveEditorReadonlyInSession",
                    "icon": "file-code",
                    "iconColor": "terminal.ansiYellow",
                    "disableTooltip": true
                },
                "Markdown: ロックされたプレビューを横に表示": {
                    "command": "markdown.showLockedPreviewToSide",
                    "icon": "file-code",
                    "iconColor": "terminal.ansiYellow",
                    "disableTooltip": true,
                    "hidden": true
                }
            }
        },
        "ファイル.履歴": {
            "nestedItems": {
                "GitHD: View Branch Diff": {
                    "command": "githd.diffBranch",
                    "icon": "history",
                    "iconColor": "terminal.ansiBrightGreen",
                    "disableTooltip": true
                }
            }
        },
        "移動.全般": {
            "nestedItems": {
                "ブラケットへ移動 【Ctrl+Shift+M】": {
                    "command": "editor.action.jumpToBracket",
                    "icon": "target",
                    "iconColor": "terminal.ansiRed",
                    "disableTooltip": true
                },
                "行/列に移動... 【Ctrl+G】": {
                    "command": "workbench.action.gotoLine",
                    "icon": "target",
                    "iconColor": "terminal.ansiRed",
                    "disableTooltip": true,
                    "hidden": true
                }
            }
        },
        "移動.履歴": {
            "nestedItems": {
                "最後の編集位置へ移動 【Ctrl+F13】【Ctrl+Shift+Backspace】": {
                    "command": "workbench.action.navigateToLastEditLocation",
                    "icon": "rocket",
                    "iconColor": "terminal.ansiBlue",
                    "disableTooltip": true
                },
                "[場所の編集] で前に戻る": {
                    "command": "workbench.action.navigateBackInEditLocations",
                    "icon": "rocket",
                    "iconColor": "terminal.ansiBlue",
                    "disableTooltip": true
                },
                "[場所の編集] で次に進む": {
                    "command": "workbench.action.navigateForwardInEditLocations",
                    "icon": "rocket",
                    "iconColor": "terminal.ansiBlue",
                    "disableTooltip": true
                },
                "[ナビゲーションの場所] で前に戻る": {
                    "command": "workbench.action.navigateBackInNavigationLocations",
                    "icon": "rocket",
                    "iconColor": "terminal.ansiBlue",
                    "disableTooltip": true
                },
                "[ナビゲーションの場所] で次に進む": {
                    "command": "workbench.action.navigateForwardInNavigationLocations",
                    "icon": "rocket",
                    "iconColor": "terminal.ansiBlue",
                    "disableTooltip": true
                }
            }
        },
        "選択.全般": {
            "nestedItems": {
                "選択範囲を拡張 【Ctrl+F17】": {
                    "command": "editor.action.smartSelect.expand",
                    "icon": "selection",
                    "iconColor": "terminal.ansiMagenta",
                    "disableTooltip": true
                },
                "選択範囲を縮小 【Ctrl+F18】": {
                    "command": "editor.action.smartSelect.shrink",
                    "icon": "selection",
                    "iconColor": "terminal.ansiMagenta",
                    "disableTooltip": true
                },
                "列選択モードの切り替え 【Ctrl+F19】": {
                    "command": "editor.action.toggleColumnSelection",
                    "icon": "selection",
                    "iconColor": "terminal.ansiMagenta",
                    "disableTooltip": true
                }
            }
        },
        "編集.全般": {
            "nestedItems": {
                "置換 【Shift+F13】【Ctrl+R】": {
                    "command": "editor.action.startFindReplaceAction",
                    "icon": "output",
                    "iconColor": "terminal.ansiYellow",
                    "disableTooltip": true
                },
                "行をつなげる 【Ctrl+Shift+J】": {
                    "command": "editor.action.joinLines",
                    "icon": "output",
                    "iconColor": "terminal.ansiYellow",
                    "disableTooltip": true
                }
            }
        },
        "編集.複製": {
            "nestedItems": {
                "行を下へコピー 【Shift+F14】【Ctrl+D】": {
                    "command": "editor.action.copyLinesDownAction",
                    "icon": "copy",
                    "iconColor": "terminal.ansiCyan",
                    "disableTooltip": true
                }
            }
        },
        "編集.移動": {
            "nestedItems": {
                "行を上へ移動 【Shift+Alt+UpArrow】": {
                    "command": "editor.action.moveLinesUpAction",
                    "icon": "go-to-file",
                    "iconColor": "terminal.ansiBrightGreen",
                    "disableTooltip": true
                },
                "行を下へ移動 【Shift+Alt+DownArrow】": {
                    "command": "editor.action.moveLinesDownAction",
                    "icon": "go-to-file",
                    "iconColor": "terminal.ansiBrightGreen",
                    "disableTooltip": true
                }
            }
        },
        "編集.変換": {
            "nestedItems": {
                "小文字に変換 【Shift+F17】": {
                    "command": "editor.action.transformToLowercase",
                    "icon": "replace",
                    "iconColor": "terminal.ansiBrightRed",
                    "disableTooltip": true
                },
                "大文字に変換 【Shift+F18】": {
                    "command": "editor.action.transformToUppercase",
                    "icon": "replace",
                    "iconColor": "terminal.ansiBrightRed",
                    "disableTooltip": true
                }
            }
        },
        "編集.フォーマット": {
            "nestedItems": {
                "ドキュメントのフォーマット 【Shift+F21】【Ctrl+Alt+L】": {
                    "command": "editor.action.formatDocument",
                    "icon": "code",
                    "iconColor": "terminal.ansiBrightCyan",
                    "disableTooltip": true
                },
                "選択範囲のフォーマット 【Shift+F22】【Ctrl+Alt+L】": {
                    "command": "editor.action.formatSelection",
                    "icon": "code",
                    "iconColor": "terminal.ansiBrightCyan",
                    "disableTooltip": true
                },
                "Format Selection (SQL Formatter VSCode)": {
                    "command": "sql-formatter-vscode.format-selection",
                    "icon": "code",
                    "iconColor": "terminal.ansiBrightCyan",
                    "disableTooltip": true
                }
            }
        },
        "編集.コメント": {
            "nestedItems": {
                "行コメントの切り替え 【Shift+F23】【Ctrl+/】": {
                    "command": "editor.action.commentLine",
                    "icon": "comment",
                    "iconColor": "terminal.ansiMagenta",
                    "disableTooltip": true
                },
                "ブロック コメントの切り替え 【Shift+F24】【Ctrl+Shift+/】": {
                    "command": "editor.action.blockComment",
                    "icon": "comment",
                    "iconColor": "terminal.ansiMagenta",
                    "disableTooltip": true
                }
            }
        },
        "編集.折りたたみ": {
            "nestedItems": {
                "Region folding: Wrap selection with #region tag.": {
                    "command": "regionfolder.wrapWithRegion",
                    "icon": "symbol-class",
                    "iconColor": "terminal.ansiGreen",
                    "disableTooltip": true
                },
                "Region folding: Delete current #region tags": {
                    "command": "regionfolder.removeCurrentRegionTags",
                    "icon": "symbol-class",
                    "iconColor": "terminal.ansiGreen",
                    "disableTooltip": true
                }
            }
        },
        "編集.Markdown": {
            "nestedItems": {
                "Markdown All in One: Create Table of Contents": {
                    "command": "markdown.extension.toc.create",
                    "icon": "markdown",
                    "iconColor": "terminal.ansiBrightRed",
                    "disableTooltip": true
                },
                "Markdown All in One: Add/Update section numbers": {
                    "command": "markdown.extension.toc.addSecNumbers",
                    "icon": "markdown",
                    "iconColor": "terminal.ansiBrightRed",
                    "disableTooltip": true
                },
                "Markdown All in One: Remove section numbers": {
                    "command": "markdown.extension.toc.removeSecNumbers",
                    "icon": "markdown",
                    "iconColor": "terminal.ansiBrightRed",
                    "disableTooltip": true
                },
                "Markdown All in One: Toggle list": {
                    "command": "markdown.extension.editing.toggleList",
                    "icon": "markdown",
                    "iconColor": "terminal.ansiBrightRed",
                    "disableTooltip": true
                },
                "Markdown All in One: Toggle code block": {
                    "command": "markdown.extension.editing.toggleCodeBlock",
                    "icon": "markdown",
                    "iconColor": "terminal.ansiBrightRed",
                    "disableTooltip": true
                }
            }
        },
        "ターミナル.全般": {
            "nestedItems": {
                "ターミナル: アクティブなターミナルで選択したテキストを実行 【Alt+F13】": {
                    "command": "workbench.action.terminal.runSelectedText",
                    "icon": "terminal",
                    "iconColor": "terminal.ansiBrightYellow",
                    "disableTooltip": true
                },
                "cygpath: WindowsのパスをUnix形式に変換": {
                    "sequence": [
                        "workbench.action.terminal.focus",
                        {
                            "command": "workbench.action.terminal.sendSequence",
                            "args": {
                                "text": "cygpath -u \"$WIN_PATH\""
                            }
                        }
                    ],
                    "icon": "terminal",
                    "iconColor": "terminal.ansiBrightYellow",
                    "disableTooltip": true
                }
            }
        },
        "ターミナル.検索": {
            "nestedItems": {
                "ls: ファイルの一覧表示（ショート版）": {
                    "sequence": [
                        "workbench.action.terminal.focus",
                        {
                            "command": "workbench.action.terminal.sendSequence",
                            "args": {
                                "text": "find $DIR -type f -exec ls -1FA --block-size=M --time-style='+%Y-%m-%dT%H:%M:%S' {} +"
                            }
                        }
                    ],
                    "icon": "terminal",
                    "iconColor": "terminal.ansiBrightCyan",
                    "disableTooltip": true
                },
                "ls: ファイルの一覧表示（ロング版）": {
                    "sequence": [
                        "workbench.action.terminal.focus",
                        {
                            "command": "workbench.action.terminal.sendSequence",
                            "args": {
                                "text": "find $DIR -type f -exec ls -lFA --block-size=M --time-style='+%Y-%m-%dT%H:%M:%S' {} +"
                            }
                        }
                    ],
                    "icon": "terminal",
                    "iconColor": "terminal.ansiBrightCyan",
                    "disableTooltip": true
                },
                "grep: 文字列の一括検索": {
                    "sequence": [
                        "workbench.action.terminal.focus",
                        {
                            "command": "workbench.action.terminal.sendSequence",
                            "args": {
                                "text": "find $DIR \\( -path '.git' $EXCLUDE_PATTERN \\) -prune -o -type f -exec grep -anHPi $SEARCH_PATTERN {} +"
                            }
                        }
                    ],
                    "icon": "terminal",
                    "iconColor": "terminal.ansiBrightCyan",
                    "disableTooltip": true
                },
                "grep: 文字列を抽出し重複削除": {
                    "sequence": [
                        "workbench.action.terminal.focus",
                        {
                            "command": "workbench.action.terminal.sendSequence",
                            "args": {
                                "text": "grep -aP '$SEARCH_PATTERN' $SEARCH_PATH | sed -E 's/^\\s+//;s/\\s+$//' | sort -u"
                            }
                        }
                    ],
                    "icon": "terminal",
                    "iconColor": "terminal.ansiBrightCyan",
                    "disableTooltip": true
                }
            }
        },
        "ターミナル.比較": {
            "nestedItems": {
                "diff: ディレクトリ間の差分比較（Windows）": {
                    "sequence": [
                        "workbench.action.terminal.focus",
                        {
                            "command": "workbench.action.terminal.sendSequence",
                            "args": {
                                "text": "diff -qr -x \"$EXCLUDE_PATTERN\" $(cygpath -u \"$DIR_1\") $(cygpath -u \"$DIR_2\")"
                            }
                        }
                    ],
                    "icon": "terminal",
                    "iconColor": "terminal.ansiRed",
                    "disableTooltip": true
                }
            }
        },
        "ターミナル.scp": {
            "nestedItems": {
                "scp: リモート→踏み台": {
                    "sequence": [
                        "workbench.action.terminal.focus",
                        {
                            "command": "workbench.action.terminal.sendSequence",
                            "args": {
                                "text": "scp $SRC_PATH $USER@$DEST_HOST:$DEST_PATH"
                            }
                        }
                    ],
                    "icon": "terminal",
                    "iconColor": "terminal.ansiBlue",
                    "disableTooltip": true
                },
                "scp: 踏み台→ローカル": {
                    "sequence": [
                        "workbench.action.terminal.focus",
                        {
                            "command": "workbench.action.terminal.sendSequence",
                            "args": {
                                "text": "scp $USER@$SRC_HOST:$SRC_PATH ~/Downloads/"
                            }
                        }
                    ],
                    "icon": "terminal",
                    "iconColor": "terminal.ansiBlue",
                    "disableTooltip": true
                }
            }
        },
        "ターミナル.ssh": {
            "nestedItems": {
                "ssh: 踏み台サーバー": {
                    "sequence": [
                        "workbench.action.terminal.focus",
                        {
                            "command": "workbench.action.terminal.sendSequence",
                            "args": {
                                "text": "ssh $USER@$REMOTE_HOST"
                            }
                        }
                    ],
                    "icon": "terminal",
                    "iconColor": "terminal.ansiMagenta",
                    "disableTooltip": true
                },
                "ssh: リモートサーバー-01": {
                    "sequence": [
                        "workbench.action.terminal.focus",
                        {
                            "command": "workbench.action.terminal.sendSequence",
                            "args": {
                                "text": "ssh $REMOTE_HOST"
                            }
                        }
                    ],
                    "icon": "terminal",
                    "iconColor": "terminal.ansiMagenta",
                    "disableTooltip": true
                },
                "ssh: リモートサーバー-02": {
                    "sequence": [
                        "workbench.action.terminal.focus",
                        {
                            "command": "workbench.action.terminal.sendSequence",
                            "args": {
                                "text": "ssh $REMOTE_HOST"
                            }
                        }
                    ],
                    "icon": "terminal",
                    "iconColor": "terminal.ansiMagenta",
                    "disableTooltip": true
                }
            }
        }
    }
}
