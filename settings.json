{
    "editor.minimap.enabled": false,
    "editor.maxTokenizationLineLength": 10000000,
    "editor.find.autoFindInSelection": "multiline",
    "editor.find.seedSearchStringFromSelection": "selection",
    "editor.renderWhitespace": "all",
    "editor.showFoldingControls": "always",
    "editor.rulers": [60, 80, 120],
    "editor.defaultFormatter": "prettier.prettier-vscode",
    "editor.detectIndentation": false,
    "search.showLineNumbers": true,
    "workbench.editor.doubleClickTabToToggleEditorGroupSizes": "maximize",
    "workbench.iconTheme": "vscode-icons",
    "problems.visibility": true,
    "window.customMenuBarAltFocus": false,
    "git.autofetch": true,
    "git.pruneOnFetch": true,
    "diffEditor.renderSideBySideInlineBreakpoint": 1,
    "diffEditor.ignoreTrimWhitespace": true,
    "diffEditor.hideUnchangedRegions.enabled": false,
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "terminal.integrated.allowChords": false,
    "terminal.integrated.sendKeybindingsToShell": true,
    "terminal.integrated.stickyScroll.enabled": false,
    "terminal.integrated.altClickMovesCursor": false,
    "npm.exclude": "",
    "github.copilot.nextEditSuggestions.enabled": true,
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": false,
        "scminput": false
    },
    "githd.editor.disabled": true,
    "githd.blameView.enabled": "disabled",
    "githd.logView.expressMode": true,
    "githd.logView.displayExpressStatus": false,
    "[oracle-sql]": {
        "editor.defaultFormatter": "Oracle.sql-developer"
    },
    "[sql]": {
        "editor.defaultFormatter": "ReneSaarsoo.sql-formatter-vsc"
    },
    "maptz.regionfolder": {
        "[plaintext]": {
            "foldStart": "#region [NAME]",
            "foldEnd": "#endregion",
            "foldStartRegex": "#region\\s*(.*)",
            "foldEndRegex": "#endregion",
            "defaultFoldStartRegex": "#region\\b",
            "disableFolding": false
        },
        "[markdown]": {
            "foldStart": "<!-- #region [NAME] -->",
            "foldEnd": "<!-- #endregion -->",
            "foldStartRegex": "<!--\\s*#region\\s*(.*)-->",
            "foldEndRegex": "<!--\\s*#endregion\\s*-->",
            "defaultFoldStartRegex": "<!--\\s*#region\\b.*-->",
            "disableFolding": false
        }
    },
    "prettier.tabWidth": 4,
    "prettier.trailingComma": "none",
    "markdown.extension.list.indentationSize": "inherit",
    "markdowntable.showMenu.csvToTable": true,
    "markdownlint.config": {
        "MD007": {
            "indent": 4
        }
    },
    "SQL-Formatter-VSCode.dialect": "plsql",
    "SQL-Formatter-VSCode.ignoreTabSettings": true,
    "SQL-Formatter-VSCode.tabSizeOverride": 4,
    "SQL-Formatter-VSCode.linesBetweenQueries": 2,
    "sqldeveloper.sqlHistory.historyLimit": 500,
    "sqldeveloper.telemetry.enabled": false,
    "sqldeveloper.format.alignment": {
        "rightAlignQueryKeywords": false,
        "namedArgumentSeparator": false
    },
    "sqldeveloper.format.indentation.identSpaces": 4,
    "jdk.telemetry.enabled": false,
    "jdk.jdkhome": "c:\\Java\\openJdk-25",
    "commands.toggleSettings.showNotification": true,
    "commands.commands": {
        "表示.全般": {
            "nestedItems": {
                "表示: [右端での折り返し] の設定/解除 【Alt+Z】": {
                    "command": "editor.action.toggleWordWrap",
                    "icon": "gear",
                    "iconColor": "terminal.ansiYellow",
                    "disableTooltip": true
                },
                "表示: エディターの分割": {
                    "command": "workbench.action.splitEditor",
                    "icon": "gear",
                    "iconColor": "terminal.ansiYellow",
                    "disableTooltip": true,
                    "hidden": true
                }
            }
        },
        "表示.レイアウト": {
            "nestedItems": {
                "表示: エディター グループのサイズをリセット 【F13】": {
                    "command": "workbench.action.evenEditorWidths",
                    "icon": "layout",
                    "iconColor": "terminal.ansiBlue",
                    "disableTooltip": true
                }
            }
        },
        "表示.スクロール": {
            "nestedItems": {
                "行の位置: トップ 【F15】": {
                    "command": "revealLine",
                    "args": {
                        "lineNumber": "${lineNumber}",
                        "at": "top"
                    },
                    "icon": "go-to-file",
                    "iconColor": "terminal.ansiRed",
                    "disableTooltip": true
                },
                "行の位置: センター 【F16】": {
                    "command": "revealLine",
                    "args": {
                        "lineNumber": "${lineNumber}",
                        "at": "center"
                    },
                    "icon": "go-to-file",
                    "iconColor": "terminal.ansiRed",
                    "disableTooltip": true
                }
            }
        },
        "表示.折りたたみ": {
            "nestedItems": {
                "選択した項目を除くすべて折りたたみ 【Ctrl+K, Ctrl+-】": {
                    "command": "editor.foldAllExcept",
                    "icon": "symbol-class",
                    "iconColor": "terminal.ansiMagenta",
                    "disableTooltip": true
                },
                "選択した項目を除くすべて展開 【Ctrl+K, Ctrl+=】": {
                    "command": "editor.unfoldAllExcept",
                    "icon": "symbol-class",
                    "iconColor": "terminal.ansiMagenta",
                    "disableTooltip": true
                },
                "すべて折りたたみ 【Ctrl+K, Ctrl+0】": {
                    "command": "editor.foldAll",
                    "icon": "symbol-class",
                    "iconColor": "terminal.ansiMagenta",
                    "disableTooltip": true
                },
                "すべて展開 【Ctrl+K, Ctrl+J】": {
                    "command": "editor.unfoldAll",
                    "icon": "symbol-class",
                    "iconColor": "terminal.ansiMagenta",
                    "disableTooltip": true
                },
                "再帰的に折りたたむ 【Ctrl+K, Ctrl+[】": {
                    "command": "editor.foldRecursively",
                    "icon": "symbol-class",
                    "iconColor": "terminal.ansiMagenta",
                    "disableTooltip": true
                },
                "再帰的に展開する 【Ctrl+K, Ctrl+]】": {
                    "command": "editor.unfoldRecursively",
                    "icon": "symbol-class",
                    "iconColor": "terminal.ansiMagenta",
                    "disableTooltip": true
                },
                "レベル 1 で折りたたむ 【Ctrl+K, Ctrl+1】": {
                    "command": "editor.foldLevel1",
                    "icon": "symbol-class",
                    "iconColor": "terminal.ansiMagenta",
                    "disableTooltip": true
                },
                "レベル 2 で折りたたむ 【Ctrl+K, Ctrl+2】": {
                    "command": "editor.foldLevel2",
                    "icon": "symbol-class",
                    "iconColor": "terminal.ansiMagenta",
                    "disableTooltip": true
                },
                "レベル 3 で折りたたむ 【Ctrl+K, Ctrl+3】": {
                    "command": "editor.foldLevel3",
                    "icon": "symbol-class",
                    "iconColor": "terminal.ansiMagenta",
                    "disableTooltip": true
                },
                "レベル 4 で折りたたむ 【Ctrl+K, Ctrl+4】": {
                    "command": "editor.foldLevel4",
                    "icon": "symbol-class",
                    "iconColor": "terminal.ansiMagenta",
                    "disableTooltip": true
                },
                "レベル 5 で折りたたむ 【Ctrl+K, Ctrl+5】": {
                    "command": "editor.foldLevel5",
                    "icon": "symbol-class",
                    "iconColor": "terminal.ansiMagenta",
                    "disableTooltip": true
                },
                "選択範囲から折りたたみ範囲を作成する 【Ctrl+K, Ctrl+,】": {
                    "command": "editor.createFoldingRangeFromSelection",
                    "icon": "symbol-class",
                    "iconColor": "terminal.ansiMagenta",
                    "disableTooltip": true
                },
                "手動折りたたみ範囲を削除する 【Ctrl+K, Ctrl+.】": {
                    "command": "editor.removeManualFoldingRanges",
                    "icon": "symbol-class",
                    "iconColor": "terminal.ansiMagenta",
                    "disableTooltip": true
                }
            }
        },
        "ファイル.全般": {
            "nestedItems": {
                "ファイル: セッションでのアクティブ エディターの読み取り専用の切り替え": {
                    "command": "workbench.action.files.toggleActiveEditorReadonlyInSession",
                    "icon": "file-code",
                    "iconColor": "terminal.ansiCyan",
                    "disableTooltip": true
                },
                "問題・警告 表示のON/OFF": {
                    "command": "commands.toggleSetting",
                    "args": {
                        "setting": "problems.visibility",
                        "value": [false, true]
                    },
                    "icon": "file-code",
                    "iconColor": "terminal.ansiCyan",
                    "disableTooltip": true
                }
            }
        },
        "ファイル.履歴": {
            "nestedItems": {
                "GitHD: View Branch Diff": {
                    "command": "githd.diffBranch",
                    "icon": "history",
                    "iconColor": "terminal.ansiGreen",
                    "disableTooltip": true
                }
            }
        },
        "移動.全般": {
            "nestedItems": {
                "ブラケットへ移動 【Ctrl+Shift+\\】": {
                    "command": "editor.action.jumpToBracket",
                    "icon": "target",
                    "iconColor": "terminal.ansiBrightYellow",
                    "disableTooltip": true
                },
                "行/列に移動... 【Ctrl+G】": {
                    "command": "workbench.action.gotoLine",
                    "icon": "target",
                    "iconColor": "terminal.ansiBrightYellow",
                    "disableTooltip": true
                }
            }
        },
        "移動.履歴": {
            "nestedItems": {
                "最後の編集位置へ移動 【Ctrl+K, Ctrl+Q】": {
                    "command": "workbench.action.navigateToLastEditLocation",
                    "icon": "rocket",
                    "iconColor": "terminal.ansiBlue",
                    "disableTooltip": true
                },
                "前に戻る 【Alt+LeftArrow】": {
                    "command": "workbench.action.navigateBack",
                    "icon": "rocket",
                    "iconColor": "terminal.ansiBlue",
                    "disableTooltip": true
                },
                "次に進む 【Alt+RightArrow】": {
                    "command": "workbench.action.navigateForward",
                    "icon": "rocket",
                    "iconColor": "terminal.ansiBlue",
                    "disableTooltip": true
                },
                "[場所の編集] で前に戻る": {
                    "command": "workbench.action.navigateBackInEditLocations",
                    "icon": "rocket",
                    "iconColor": "terminal.ansiBlue",
                    "disableTooltip": true
                },
                "[場所の編集] で次に進む": {
                    "command": "workbench.action.navigateForwardInEditLocations",
                    "icon": "rocket",
                    "iconColor": "terminal.ansiBlue",
                    "disableTooltip": true
                },
                "[ナビゲーションの場所] で前に戻る": {
                    "command": "workbench.action.navigateBackInNavigationLocations",
                    "icon": "rocket",
                    "iconColor": "terminal.ansiBlue",
                    "disableTooltip": true
                },
                "[ナビゲーションの場所] で次に進む": {
                    "command": "workbench.action.navigateForwardInNavigationLocations",
                    "icon": "rocket",
                    "iconColor": "terminal.ansiBlue",
                    "disableTooltip": true
                }
            }
        },
        "選択.全般": {
            "nestedItems": {
                "選択範囲を拡張 【Shift+Alt+RightArrow】": {
                    "command": "editor.action.smartSelect.expand",
                    "icon": "selection",
                    "iconColor": "terminal.ansiRed",
                    "disableTooltip": true
                },
                "選択範囲を縮小 【Shift+Alt+LeftArrow】": {
                    "command": "editor.action.smartSelect.shrink",
                    "icon": "selection",
                    "iconColor": "terminal.ansiRed",
                    "disableTooltip": true
                },
                "列選択モードの切り替え 【Ctrl+F13】": {
                    "command": "editor.action.toggleColumnSelection",
                    "icon": "selection",
                    "iconColor": "terminal.ansiRed",
                    "disableTooltip": true
                }
            }
        },
        "編集.削除": {
            "nestedItems": {
                "行の削除 【Ctrl+Shift+K】": {
                    "command": "editor.action.deleteLines",
                    "icon": "clear-all",
                    "iconColor": "terminal.ansiMagenta",
                    "disableTooltip": true
                }
            }
        },
        "編集.挿入": {
            "nestedItems": {
                "行を上に挿入 【Ctrl+Shift+Enter】": {
                    "command": "editor.action.insertLineBefore",
                    "icon": "insert",
                    "iconColor": "terminal.ansiCyan",
                    "disableTooltip": true
                },
                "行を下に挿入 【Ctrl+Enter】": {
                    "command": "editor.action.insertLineAfter",
                    "icon": "insert",
                    "iconColor": "terminal.ansiCyan",
                    "disableTooltip": true
                }
            }
        },
        "編集.複製": {
            "nestedItems": {
                "行を上へコピー 【Shift+Alt+UpArrow】": {
                    "command": "editor.action.copyLinesUpAction",
                    "icon": "clippy",
                    "iconColor": "terminal.ansiGreen",
                    "disableTooltip": true
                },
                "行を下へコピー 【Shift+Alt+DownArrow】": {
                    "command": "editor.action.copyLinesDownAction",
                    "icon": "clippy",
                    "iconColor": "terminal.ansiGreen",
                    "disableTooltip": true
                }
            }
        },
        "編集.移動": {
            "nestedItems": {
                "行を上へ移動 【Alt+UpArrow】": {
                    "command": "editor.action.moveLinesUpAction",
                    "icon": "go-to-file",
                    "iconColor": "terminal.ansiYellow",
                    "disableTooltip": true
                },
                "行を下へ移動 【Alt+DownArrow】": {
                    "command": "editor.action.moveLinesDownAction",
                    "icon": "go-to-file",
                    "iconColor": "terminal.ansiYellow",
                    "disableTooltip": true
                }
            }
        },
        "編集.置換": {
            "nestedItems": {
                "置換 【Ctrl+H】": {
                    "command": "editor.action.startFindReplaceAction",
                    "icon": "replace-all",
                    "iconColor": "terminal.ansiBlue",
                    "disableTooltip": true
                }
            }
        },
        "編集.インデント": {
            "nestedItems": {
                "行のインデント 【Ctrl+]】": {
                    "command": "editor.action.indentLines",
                    "icon": "indent",
                    "iconColor": "terminal.ansiRed",
                    "disableTooltip": true
                },
                "行のインデント解除 【Ctrl+[】": {
                    "command": "editor.action.outdentLines",
                    "icon": "indent",
                    "iconColor": "terminal.ansiRed",
                    "disableTooltip": true
                }
            }
        },
        "編集.フォーマット": {
            "nestedItems": {
                "ドキュメントのフォーマット 【Shift+Alt+F】": {
                    "command": "editor.action.formatDocument",
                    "icon": "code",
                    "iconColor": "terminal.ansiMagenta",
                    "disableTooltip": true
                },
                "選択範囲のフォーマット 【Ctrl+K, Ctrl+F】": {
                    "command": "editor.action.formatSelection",
                    "icon": "code",
                    "iconColor": "terminal.ansiMagenta",
                    "disableTooltip": true
                },
                "Format Selection (SQL Formatter VSCode)": {
                    "command": "sql-formatter-vscode.format-selection",
                    "icon": "code",
                    "iconColor": "terminal.ansiMagenta",
                    "disableTooltip": true
                }
            }
        },
        "編集.コメント": {
            "nestedItems": {
                "行コメントの切り替え 【Ctrl+/】": {
                    "command": "editor.action.commentLine",
                    "icon": "comment",
                    "iconColor": "terminal.ansiCyan",
                    "disableTooltip": true
                },
                "ブロック コメントの切り替え 【Shift+Alt+A】": {
                    "command": "editor.action.blockComment",
                    "icon": "comment",
                    "iconColor": "terminal.ansiCyan",
                    "disableTooltip": true
                }
            }
        },
        "編集.変換": {
            "nestedItems": {
                "小文字に変換": {
                    "command": "editor.action.transformToLowercase",
                    "icon": "replace",
                    "iconColor": "terminal.ansiGreen",
                    "disableTooltip": true
                },
                "大文字に変換": {
                    "command": "editor.action.transformToUppercase",
                    "icon": "replace",
                    "iconColor": "terminal.ansiGreen",
                    "disableTooltip": true
                }
            }
        },
        "編集.その他": {
            "nestedItems": {
                "行をつなげる": {
                    "command": "editor.action.joinLines",
                    "icon": "output",
                    "iconColor": "terminal.ansiBrightYellow",
                    "disableTooltip": true
                }
            }
        },
        "編集.Markdown": {
            "nestedItems": {
                "Markdown All in One: 目次（TOC）の作成": {
                    "command": "markdown.extension.toc.create",
                    "icon": "markdown",
                    "iconColor": "terminal.ansiBlue",
                    "disableTooltip": true
                },
                "Markdown All in One: 目次（TOC）の更新": {
                    "command": "markdown.extension.toc.update",
                    "icon": "markdown",
                    "iconColor": "terminal.ansiBlue",
                    "disableTooltip": true
                },
                "Markdown All in One: セクション番号を追加/更新": {
                    "command": "markdown.extension.toc.addSecNumbers",
                    "icon": "markdown",
                    "iconColor": "terminal.ansiBlue",
                    "disableTooltip": true
                },
                "Markdown All in One: セクション番号の除去": {
                    "command": "markdown.extension.toc.removeSecNumbers",
                    "icon": "markdown",
                    "iconColor": "terminal.ansiBlue",
                    "disableTooltip": true
                },
                "Markdown All in One: リスト構文のトグル": {
                    "command": "markdown.extension.editing.toggleList",
                    "icon": "markdown",
                    "iconColor": "terminal.ansiBlue",
                    "disableTooltip": true
                },
                "Markdown All in One: コードブロック構文のトグル": {
                    "command": "markdown.extension.editing.toggleCodeBlock",
                    "icon": "markdown",
                    "iconColor": "terminal.ansiBlue",
                    "disableTooltip": true
                },
                "Markdown All in One: インラインコード構文のトグル": {
                    "command": "markdown.extension.editing.toggleCodeSpan",
                    "icon": "markdown",
                    "iconColor": "terminal.ansiBlue",
                    "disableTooltip": true
                },
                "Markdown All in One: Toggle Bold 【Ctrl+B】": {
                    "command": "markdown.extension.editing.toggleBold",
                    "icon": "markdown",
                    "iconColor": "terminal.ansiBlue",
                    "disableTooltip": true
                },
                "Markdown All in One: Toggle Italic 【Ctrl+I】": {
                    "command": "markdown.extension.editing.toggleItalic",
                    "icon": "markdown",
                    "iconColor": "terminal.ansiBlue",
                    "disableTooltip": true
                },
                "Markdown All in One: Toggle Strikethrough 【Alt+S】": {
                    "command": "markdown.extension.editing.toggleStrikethrough",
                    "icon": "markdown",
                    "iconColor": "terminal.ansiBlue",
                    "disableTooltip": true
                },
                "Toggle heading (uplevel) 【Ctrl+Shift+]】": {
                    "command": "markdown.extension.editing.toggleHeadingUp",
                    "icon": "markdown",
                    "iconColor": "terminal.ansiBlue",
                    "disableTooltip": true
                },
                "Toggle heading (downlevel) 【Ctrl+Shift+[】": {
                    "command": "markdown.extension.editing.toggleHeadingDown",
                    "icon": "markdown",
                    "iconColor": "terminal.ansiBlue",
                    "disableTooltip": true
                },
                "md2confl: Convert markdown to confluence and Copy to clipboard": {
                    "command": "extension.md2confl.convert2Clipboard",
                    "icon": "markdown",
                    "iconColor": "terminal.ansiBlue",
                    "disableTooltip": true
                },
                "目次・セクションから除外": {
                    "command": "editor.action.insertSnippet",
                    "args": {
                        "snippet": "<!-- omit from toc -->"
                    },
                    "icon": "markdown",
                    "iconColor": "terminal.ansiBlue",
                    "disableTooltip": true
                }
            }
        },
        "編集.折りたたみ": {
            "nestedItems": {
                "Region folding: Wrap selection with #region tag.": {
                    "command": "regionfolder.wrapWithRegion",
                    "icon": "symbol-class",
                    "iconColor": "terminal.ansiRed",
                    "disableTooltip": true
                },
                "Region folding: Delete current #region tags": {
                    "command": "regionfolder.removeCurrentRegionTags",
                    "icon": "symbol-class",
                    "iconColor": "terminal.ansiRed",
                    "disableTooltip": true
                }
            }
        },
        "ターミナル.全般": {
            "nestedItems": {
                "ターミナル: アクティブなターミナルで選択したテキストを実行 【Alt+F13】": {
                    "command": "workbench.action.terminal.runSelectedText",
                    "icon": "terminal",
                    "iconColor": "terminal.ansiMagenta",
                    "disableTooltip": true
                },
                "cygpath: WindowsのパスをUnix形式に変換": {
                    "sequence": [
                        "workbench.action.terminal.focus",
                        {
                            "command": "workbench.action.terminal.sendSequence",
                            "args": {
                                "text": "cygpath -u \"$WIN_PATH\""
                            }
                        }
                    ],
                    "icon": "terminal",
                    "iconColor": "terminal.ansiMagenta",
                    "disableTooltip": true
                }
            }
        },
        "ターミナル.検索": {
            "nestedItems": {
                "ls: ファイルの一覧表示（ショート版）": {
                    "sequence": [
                        "workbench.action.terminal.focus",
                        {
                            "command": "workbench.action.terminal.sendSequence",
                            "args": {
                                "text": "find $DIR -type f -exec ls -1FA --block-size=M --time-style='+%Y-%m-%dT%H:%M:%S' {} +"
                            }
                        }
                    ],
                    "icon": "terminal",
                    "iconColor": "terminal.ansiCyan",
                    "disableTooltip": true
                },
                "ls: ファイルの一覧表示（ロング版）": {
                    "sequence": [
                        "workbench.action.terminal.focus",
                        {
                            "command": "workbench.action.terminal.sendSequence",
                            "args": {
                                "text": "find $DIR -type f -exec ls -lFA --block-size=M --time-style='+%Y-%m-%dT%H:%M:%S' {} +"
                            }
                        }
                    ],
                    "icon": "terminal",
                    "iconColor": "terminal.ansiCyan",
                    "disableTooltip": true
                },
                "grep: 文字列の一括検索": {
                    "sequence": [
                        "workbench.action.terminal.focus",
                        {
                            "command": "workbench.action.terminal.sendSequence",
                            "args": {
                                "text": "find $DIR \\( -path '.git' $EXCLUDE_PATTERN \\) -prune -o -type f -exec grep -anHPi $SEARCH_PATTERN {} +"
                            }
                        }
                    ],
                    "icon": "terminal",
                    "iconColor": "terminal.ansiCyan",
                    "disableTooltip": true
                },
                "grep: 文字列を抽出し重複削除": {
                    "sequence": [
                        "workbench.action.terminal.focus",
                        {
                            "command": "workbench.action.terminal.sendSequence",
                            "args": {
                                "text": "grep -aP '$SEARCH_PATTERN' $SEARCH_PATH | sed -E 's/^\\s+//;s/\\s+$//' | sort -u"
                            }
                        }
                    ],
                    "icon": "terminal",
                    "iconColor": "terminal.ansiCyan",
                    "disableTooltip": true
                }
            }
        },
        "ターミナル.比較": {
            "nestedItems": {
